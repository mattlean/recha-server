{
  "name": "recha-server",
  "version": "0.0.1",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "build": "webpack --env production",
    "build:debug": "node --inspect-brk node_modules/.bin/webpack --env production",
    "build:dev": "webpack --env development",
    "build:dev:debug": "node --inspect-brk node_modules/.bin/webpack --env development",
    "build:dev:stats": "yarn run build:dev -- --json > stats.development.json",
    "build:stats": "yarn run build -- --json > stats.production.json",
    "format": "prettier --write --parser=typescript --print-width=120 --semi=false --single-quote \"{config,src}/**/*.{js,ts}\"",
    "lint": "eslint --cache \"{config,src}/**/*.{js,ts}\"",
    "start": "node build/production/server.js",
    "start:debug": "node --inspect-brk build/production/server.js",
    "start:dev": "node build/development/server.js",
    "start:dev:debug": "node --inspect-brk build/development/server.js",
    "start:dev:watch": "nodemon --exec \"yarn run build:dev && yarn run start:dev\"",
    "start:dev:watch:debug": "nodemon --exec \"yarn run build:dev && yarn run start:dev:debug\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacLean/recha-server.git"
  },
  "author": "Matthew Lean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IsaacLean/recha-server/issues"
  },
  "homepage": "https://github.com/IsaacLean/recha-server#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "checkpointjs": "^0.0.5",
    "compression": "^1.7.3",
    "cross-fetch": "^3.0.1",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "moment": "^2.24.0",
    "pg-promise": "^8.5.6",
    "source-map-support": "^0.5.10",
    "validator": "^13.7.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.42",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "@typescript-eslint/eslint-plugin": "^1.4.1",
    "@typescript-eslint/parser": "^1.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-airbnb": "^3.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "source-map-loader": "^0.2.4",
    "supertest": "^3.4.2",
    "terser-webpack-plugin": "^1.2.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
